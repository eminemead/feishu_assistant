{
  "name": "ai-sdk-feishu-agent",
  "version": "0.1.0",
  "description": "AI-powered Feishu/Lark agent using Vercel AI SDK",
  "main": "dist/server.js",
  "packageManager": "bun@latest",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:transpile": "npx esbuild server.ts --platform=node --target=es2020 --outdir=dist --bundle --format=cjs --external:@larksuiteoapi/node-sdk --external:duckdb --external:canvas --external:sharp --external:postgres --external:@ai-sdk-tools/artifacts --external:@ai-sdk-tools/agents --external:@ai-sdk-tools/cache --external:@ai-sdk-tools/memory --external:@ai-sdk-tools/store --external:@ai-sdk-tools/devtools --external:@observablehq/plot --external:jsdom",
    "build": "bun run build:transpile",
    "build:tsc": "NODE_OPTIONS='--max-old-space-size=8192' bun tsc",
    "build:fast": "bun build ./server.ts --outdir ./dist --target node --external @larksuiteoapi/node-sdk --external duckdb --external canvas --external sharp",
    "build:watch": "bun tsc --watch",
    "build:clean": "rm -rf dist .tsbuildinfo && bun run build",
    "start": "bun dist/server.js",
    "dev": "bun server.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:phoenix-trace": "bun scripts/test-phoenix-trace.ts",
    "setup:supabase": "bun scripts/setup-supabase.ts",
    "migrate:supabase": "bun scripts/run-migrations.ts",
    "test:supabase": "bun scripts/test-supabase.ts",
    "permissions:set": "bun scripts/manage-user-permissions.ts set",
    "permissions:get": "bun scripts/manage-user-permissions.ts get",
    "permissions:list": "bun scripts/manage-user-permissions.ts list",
    "test:lark-mcp": "bun scripts/test-lark-mcp-simple.ts"
  },
  "keywords": ["feishu", "lark", "ai", "chatbot"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk-tools/agents": "^1.0.8",
    "@ai-sdk-tools/artifacts": "^1.0.8",
    "@ai-sdk-tools/cache": "^1.0.8",
    "@ai-sdk-tools/memory": "^1.0.8",
    "@ai-sdk-tools/store": "^1.0.8",
    "@hono/node-server": "^1.12.0",
    "@larksuiteoapi/lark-mcp": "^0.5.1",
    "@larksuiteoapi/node-sdk": "^1.44.0",
    "@mastra/arize": "^1.0.0-beta.5",
    "@mastra/core": "^1.0.0-beta.14",
    "@mastra/hono": "^0.0.0-trace-timeline-update-20251121092347",
    "@mastra/memory": "^1.0.0-beta.4",
    "@mastra/observability": "^1.0.0-beta.4",
    "@mastra/pg": "^1.0.0-beta.5",
    "@mastra/rag": "^2.0.0-beta.3",
    "@observablehq/plot": "^0.6.17",
    "@openrouter/ai-sdk-provider": "^1.2.3",
    "@supabase/supabase-js": "^2.81.1",
    "agentfs-sdk": "^0.3.1",
    "ai": "^5.0.93",
    "canvas": "^3.2.0",
    "drizzle-orm": "^0.44.7",
    "duckdb": "^1.0.0",
    "exa-js": "^1.4.10",
    "hono": "^4.5.0",
    "jsdom": "^27.2.0",
    "jsonwebtoken": "^9.0.2",
    "just-bash": "^1.0.1",
    "mysql2": "^3.15.3",
    "postgres": "^3.4.7",
    "postgres-js": "^0.1.0",
    "sharp": "^0.34.5",
    "yaml": "^2.8.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@ai-sdk-tools/devtools": "^1.0.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.17",
    "esbuild": "^0.20.2",
    "typescript": "^5.3.3"
  }
}
